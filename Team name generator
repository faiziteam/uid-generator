    <title>Random Name Generator</title>
    <style>
        /* Styling for the generator */
        .name-generator-container {
            font-family: Arial, sans-serif;
            text-align: center;
            background-color: #f9f9f9;
            padding: 20px;
            max-width: 400px;
            margin: 20px auto;
            border-radius: 8px;
            box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1);
            border: 1px solid #ddd;
        }

        .name-generator-container h1 {
            font-size: 1.8rem;
            margin-bottom: 15px;
            color: #444;
        }

        #nameDisplay {
            font-size: 1.2rem;
            font-weight: bold;
            margin: 15px 0;
            color: #0073e6;
        }

        button {
            padding: 10px 20px;
            font-size: 0.9rem;
            color: white;
            background-color: #0073e6;
            border: none;
            border-radius: 5px;
            cursor: pointer;
        }

        button:hover {
            background-color: #005bb5;
        }
    </style>
