<body>
    <div class="name-generator-container">
        <h1>Random Name Generator</h1>
        <p id="nameDisplay">Click the button to generate a name.</p>
        <button onclick="displayGeneratedName()">Generate Name</button>
    </div>

    <script>
        const names = [
            "Hootie", "Oliver", "Athena", "Noctis", "Screech", "Nimbus", "Talon", 
            "Whisper", "Luna", "Eclipse", "Twilight", "Echo", "Glimmer", "Zephyr", 
            "Aurora", "Haven", "Orion", "Sage", "Willow", "Solstice", "Midnight", 
            "Ember", "Starry", "Ivy", "Chime", "Nova", "Shadow", "Owliver", 
            "Spirit", "Moonbeam", "Raven", "Sparkle", "Ash", "Pearl", "Frost", 
            "Flit", "Maple", "Velvet", "Fable", "Jet", "Meadow", "Quartz", 
            "Gossamer", "Fern", "Comet", "Winter", "Wisp", "Tundra", "Misty", 
            "Halo"
        ];

        function generateName() {
            const randomIndex = Math.floor(Math.random() * names.length);
            return names[randomIndex];
        }

        function displayGeneratedName() {
            document.getElementById('nameDisplay').innerText = generateName();
        }
    </script>
</body>
</html>
